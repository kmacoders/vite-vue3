{
  "name": "vite-vue3",
  "version": "1.0.0",
  "description": "Starter for Vue 3 with Vite",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "docs": "npx typedoc"
  },
  "keywords": [
    "vite",
    "vue3"
  ],
  "author": "Rio Phung",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/kmacoders/vite-vue3.git"
  },
  "homepage": "https://github.com/kmacoders/vite-vue3",
  "bugs": {
    "url": "https://github.com/kmacoders/vite-vue3/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/node": "^16.3.3",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
